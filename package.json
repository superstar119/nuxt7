{
  "name": "nuxt7",
  "version": "1.0.0",
  "license": "MIT",
  "main": "lib/module.js",
  "scripts": {
    "dev": "npm run ks",
    "ks": "nuxt dev examples/kitchen-sink",
    "ks-build": "nuxt build examples/kitchen-sink",
    "ks-analyze": "nuxt build --analyze examples/kitchen-sink",
    "ks-start": "nuxt start --spa examples/kitchen-sink",
    "ks-generate": "nuxt generate examples/kitchen-sink",
    "lint": "eslint --ext .js,.vue .",
    "test": "npm run lint",
    "precommit": "lint-staged",
    "surge": "surge -d https://nuxt7.cf examples/kitchen-sink/dist",
    "release": "standard-version && git push --follow-tags && npm publish"
  },
  "lint-staged": {
    "*.{js,vue}": ["yarn lint"]
  },
  "dependencies": {
    "@nuxtjs/pwa": "^2.0.5",
    "framework7": "^2.0.6",
    "framework7-vue": "^2.0.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "nuxt": "^1.1.1",
    "standard-version": "^4.3.0",
    "surge": "^0.19.0"
  }
}
